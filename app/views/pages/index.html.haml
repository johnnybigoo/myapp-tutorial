    / CONTACT
    %section#contact
      .container
        %header.text-center.mb-60
          %h2 Contact Us
          %p.lead.font-lato It will be our pleasure to answer questions that you might have.
          %hr/
        .row
          / FORM
          .col-md-8.col-sm-8
            %h3
              Drop us a line or just say
              %strong
                %em Hello!
            / Alert Success
            #alert_success.alert.alert-success.mb-30
              %button.close{"aria-hidden" => "true", "data-dismiss" => "alert", type: "button"} ×
              %strong Thank You!
              Your message successfully sent!
            / /Alert Success
            / Alert Failed
            #alert_failed.alert.alert-danger.mb-30
              %button.close{"aria-hidden" => "true", "data-dismiss" => "alert", type: "button"} ×
              %strong [SMTP] Error!
              Internal server error!
            / /Alert Failed
            / Alert Mandatory
            #alert_mandatory.alert.alert-danger.mb-30
              %button.close{"aria-hidden" => "true", "data-dismiss" => "alert", type: "button"} ×
              %strong Sorry!
              You need to complete all mandatory (*) fields!
            / /Alert Mandatory
            %form{action: "php/contact.php", enctype: "multipart/form-data", method: "post"}
              %fieldset
                %input{name: "action", type: "hidden", value: "contact_send"}/
                .row
                  .col-md-4
                    %label{for: "contact:name"} Full Name *
                    %input#contact:name.form-control{name: "contact[name][required]", required: "", type: "text", value: ""}/
                  .col-md-4
                    %label{for: "contact:email"} E-mail Address *
                    %input#contact:email.form-control{name: "contact[email][required]", required: "", type: "email", value: ""}/
                  .col-md-4
                    %label{for: "contact:phone"} Phone
                    %input#contact:phone.form-control{name: "contact[phone]", type: "text", value: ""}/
                .row
                  .col-md-4
                    %label{for: "contact:name"} Business Name *
                    %input#contact:business.form-control{name: "contact[name][required]", required: "", type: "text", value: ""}/
                  .col-md-8
                    %label{for: "contact:name"} Project Name *
                    %input#contact:project.form-control{name: "contact[email][required]", required: "", type: "name", value: ""}/
                .row
                  .col-md-8
                    %label{for: "contact:subject"} Project Description *
                    %input#contact:subject.form-control{name: "contact[subject][required]", required: "", type: "text", value: ""}/
                  .col-md-4
                    %label{for: "contact_department"} Department
                    %select.form-control.pointer{name: "contact[department]"}
                      %option{value: ""} --- Select ---
                      %option{value: "Marketing"} Residential
                      %option{value: "Webdesign"} Commercial &amp; Corporate
                      %option{value: "Architecture"} Maintenance
                .row
                  .col-md-12
                    %label{for: "contact:message"} Message *
                    %textarea#contact:message.form-control{maxlength: "10000", name: "contact[message]", required: "", rows: "8"}
                .row
                  .col-md-12
                    %label{for: "contact:attachment"} File Attachment
                    / custom file upload
                    %input#file.custom-file-upload{"data-btn-text" => "Select a File", name: "contact[attachment]", type: "file"}/
                    %small.text-muted.block Max file size: 10Mb (zip/pdf/jpg/png)
              .row
                .col-md-12
                  %button.btn.btn-primary{type: "submit"}
                    %i.fa.fa-check
                    SEND MESSAGE
          / /FORM
